{
	"name": "react-designer",
	"version": "1.1.1",
	"description": "Easy to configure, lightweight, editable vector graphics in your react components.",
	"main": "dist/index.js",
	"scripts": {
		"start": "webpack-dev-server --mode development",
		"test": "echo \"Error: no test specified\" && exit 1",
		"transpile": "babel src -d dist --copy-files",
		"prePublishOnly": "npm run transpile",
		"build": "webpack --mode production",
		"deploy": "gh-pages -d examples/dist",
		"publish-demo": "npm run build && npm run deploy",
		"format": "prettier --write \"src/**/*.js\"",
		"lint": "eslint --fix \"src/**/*.js\""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fatiherikli/react-designer"
	},
	"keywords": [
		"react",
		"reactjs"
	],
	"author": "Fatih Erikli <fatiherikli@gmail.com> (http://fatiherikli.com)",
	"license": "MIT",
	"homepage": "https://github.com/react-designer/react-designer",
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/core": "^7.15.8",
		"@babel/node": "^7.15.8",
		"@babel/plugin-proposal-export-default-from": "^7.14.5",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-react": "^7.14.5",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"css-loader": "^6.4.0",
		"eslint": "^7.32.0",
		"eslint-plugin-react": "^7.26.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.3.2",
		"husky": "^7.0.2",
		"prettier": "2.4.1",
		"react": "^16.8.0",
		"react-autocomplete": "1.8.1",
		"react-dom": "^16.8.0",
		"react-dropzone": "^11.4.2",
		"react-hot-loader": "^4.13.0",
		"rimraf": "^3.0.2",
		"sass": "^1.43.2",
		"sass-loader": "^12.2.0",
		"style-loader": "^3.3.0",
		"superagent": "*",
		"svg-inline-loader": "^0.8.2",
		"url-loader": "^4.1.1",
		"webfontloader": "^1.6.28",
		"webpack": "^5.58.1",
		"webpack-cli": "^4.9.0",
		"webpack-dev-server": "^4.3.1"
	},
	"peerDependencies": {
		"react": "^16.8.0",
		"react-dom": "^16.8.0"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"lodash": ">=4.17.11",
		"prop-types": "^15.7.2",
		"react-hotkeys": "^1.1.4",
		"react-portal": "^4.2.0",
		"webfontloader": "^1.6.28"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format"
		}
	}
}
